<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Document</title>
    <style>
      .avaPic{
        width: 300px;
        height: 300px;
        border-radius: 150px;
        display: block;
        margin-left: auto;
        margin-right: auto;
      }
      .nameAccount{
        display: flex;
        justify-content: center;
        font-size: 32px;
      }
    </style>
</head>
<body>
    <header>
      <%- include('../partials/header') %>
    </header>
    <div class="profileAccount">
      <% for(var i = 0; i < users.length; i++) { %>
        <div>
          <img src="/<%= users[i].profilePicture %>" class="avaPic">
        </div>
        <div>
          <p class="nameAccount" ><%= users[i].fullName %></p>
        </div>
        <% } %>
    </div>
    
    <footer>
        <%- include('../partials/footer') %>
      </footer>
    
</body>
</html>